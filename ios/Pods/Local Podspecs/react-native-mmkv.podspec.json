{
  "name": "react-native-mmkv",
  "version": "3.3.1",
  "summary": "The fastest key/value storage for React Native. ~30x faster than AsyncStorage! Works on Android, iOS and Web.",
  "homepage": "https://github.com/mrousavy/react-native-mmkv#readme",
  "license": "(MIT AND BSD-3-Clause)",
  "authors": "Marc Rousavy <me@mrousavy.com> (https://github.com/mrousavy)",
  "platforms": {
    "ios": "15.1",
    "tvos": "12.0",
    "osx": "10.14"
  },
  "source": {
    "git": "https://github.com/mrousavy/react-native-mmkv.git",
    "tag": "3.3.1"
  },
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "CLANG_CXX_LIBRARY": "libc++",
    "CLANG_WARN_OBJC_IMPLICIT_RETAIN_SELF": "NO",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) FORCE_POSIX",
    "HEADER_SEARCH_PATHS": [
      "\"$(PODS_ROOT)/Headers/Private/Yoga\"",
      "$(PODS_ROOT)/glog",
      "$(PODS_ROOT)/boost",
      "$(PODS_ROOT)/DoubleConversion",
      "$(PODS_ROOT)/fast_float/include",
      "$(PODS_ROOT)/fmt/include",
      "$(PODS_ROOT)/SocketRocket",
      "$(PODS_ROOT)/RCT-Folly"
    ],
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 ",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  },
  "compiler_flags": "-x objective-c++  -DRCT_NEW_ARCH_ENABLED=1",
  "libraries": [
    "z",
    "c++"
  ],
  "source_files": [
    "ios/**/*.{h,m,mm}",
    "cpp/**/*.{hpp,cpp,c,h}",
    "MMKV/Core/**/*.{h,cpp,hpp,S}"
  ],
  "dependencies": {
    "React-Core": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "React-jsi": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "React-hermes": [],
    "glog": [],
    "boost": [],
    "DoubleConversion": [],
    "fast_float": [],
    "fmt": [],
    "RCT-Folly": [],
    "SocketRocket": [],
    "RCT-Folly/Fabric": []
  }
}
